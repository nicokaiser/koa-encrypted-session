{
  "name": "koa-encrypted-session",
  "version": "3.0.4",
  "description": "Encrypted cookie session middleware extension for koa-session",
  "homepage": "https://github.com/nicokaiser/koa-encrypted-session",
  "repository": {
    "type": "git",
    "url": "git://github.com/nicokaiser/koa-encrypted-session.git"
  },
  "keywords": [
    "koa",
    "middleware",
    "session",
    "cookie",
    "encrypted"
  ],
  "files": [
    "index.js",
    "lib/util.js"
  ],
  "main": "index.js",
  "bin": {
    "koa-encrypted-session-gen-key": "gensecretkey.js"
  },
  "dependencies": {
    "base64url": "^3.0.1",
    "koa-session": "^6.2.0",
    "sodium-native": "^3.3.0"
  },
  "devDependencies": {
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "koa": "^2.13.4",
    "mocha": "^10.0.0",
    "prettier": "^2.6.2",
    "should": "^13.2.3",
    "supertest": "^6.2.3"
  },
  "scripts": {
    "lint": "eslint index.js test index.js",
    "test": "npm run lint && NODE_ENV=test mocha --exit --require should --reporter spec test/*.test.js"
  },
  "engines": {
    "node": ">=12"
  },
  "author": "Nico Kaiser <nico@kaiser.me>",
  "license": "MIT",
  "prettier": {
    "singleQuote": true
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended",
      "prettier"
    ],
    "env": {
      "node": true,
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 2018
    },
    "overrides": [
      {
        "files": [
          "test/**/*.js"
        ],
        "env": {
          "mocha": true
        }
      }
    ]
  }
}
